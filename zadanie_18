 <script language="JavaScript">
 var a=parseFloat(prompt("a=",""));
 var b=parseFloat(prompt("b=",""));
 function suma_łuk(a, b) {
 return a + b;
 }
 document.write("suma="+suma_łuk(a,b)+"<br>");
</script>
